{
	"provider": {
		"name": "panther"
	},
	"resources": [
		{
			"name": "httpsource",
			"schema": {
				"attributes": [
					{
						"name": "integration_label",
						"string": {
							"computed_optional_required": "required",
							"description": "The id of the data model"
						}
					},
					{
						"name": "log_stream_type",
						"string": {
							"computed_optional_required": "required",
							"description": "The log stream types",
							"validators": [
								{
									"custom": {
										"imports": [
											{
												"path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
											}
										],
										"schema_definition": "stringvalidator.OneOf(\n\"Auto\",\n\"CloudWatchLogs\",\n\"JSON\",\n\"JsonArray\",\n\"Lines\",\n)"
									}
								}
							]
						}
					},
					{
						"name": "log_types",
						"list": {
							"computed_optional_required": "required",
							"element_type": {
								"string": {}
							},
							"description": "The log types of the integration"
						}
					},
					{
						"name": "security_alg",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "The authentication algorithm of the http source"
						}
					},
					{
						"name": "security_header_key",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "The security header key of the http source"
						}
					},
					{
						"name": "security_password",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "The authentication header password of the http source"
						}
					},
					{
						"name": "security_secret_value",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "The header secret value of the http source"
						}
					},
					{
						"name": "security_type",
						"string": {
							"computed_optional_required": "required",
							"description": "The security type of the http endpoint",
							"validators": [
								{
									"custom": {
										"imports": [
											{
												"path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
											}
										],
										"schema_definition": "stringvalidator.OneOf(\n\"SharedSecret\",\n\"HMAC\",\n\"Bearer\",\n\"Basic\",\n\"None\",\n)"
									}
								}
							]
						}
					},
					{
						"name": "security_username",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "The authentication header username of the http source"
						}
					},
					{
						"name": "integration_id",
						"string": {
							"computed_optional_required": "computed",
							"description": "The id of the data model"
						}
					}
				]
			}
		}
	],
	"version": "0.1"
}